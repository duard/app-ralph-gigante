'use client';

import { useEffect, useState } from 'react';
import { Link, useSearchParams } from 'react-router-dom';
import { BaseLayout } from '@/components/layouts/base-layout';
import { Card, CardContent, CardDescription, CardHeader, CardTitle } from '@/components/ui/card';
import { Badge } from '@/components/ui/badge';
import { Input } from '@/components/ui/input';
import { Button } from '@/components/ui/button';
import {
  Table,
  TableBody,
  TableCell,
  TableHead,
  TableHeader,
  TableRow,
} from '@/components/ui/table';
import {
  Select,
  SelectContent,
  SelectItem,
  SelectTrigger,
  SelectValue,
} from '@/components/ui/select';
import {
  Tooltip,
  TooltipContent,
  TooltipProvider,
  TooltipTrigger,
} from '@/components/ui/tooltip';
import {
  Package,
  Search,
  AlertTriangle,
  Loader2,
  ChevronLeft,
  ChevronRight,
  Filter,
  X,
  FileWarning,
  CheckCircle,
} from 'lucide-react';
import {
  getProdutosSemNCM,
  getStatsProdutosSemNCM,
  getGruposProdutos,
  getLocaisEstoque,
  calcularPercentual,
  type ProdutosSemNCMFilters,
  type ProdutosSemNCMResponse,
  type ProdutosSemNCMStats,
  type GrupoProduto,
  type LocalEstoque,
} from '@/lib/api/qualidade-service';

export default function SemNCMPage() {
  const [searchParams, setSearchParams] = useSearchParams();
  const [data, setData] = useState<ProdutosSemNCMResponse | null>(null);
  const [stats, setStats] = useState<ProdutosSemNCMStats | null>(null);
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState<string | null>(null);

  // Ler filtros da URL ou usar defaults (padrão: mostrar ativos)
  const [search, setSearch] = useState(searchParams.get('search') || '');
  const [ativo, setAtivo] = useState<'S' | 'N' | 'all'>((searchParams.get('ativo') as 'S' | 'N' | 'all') || 'S');
  const [codgrupoprod, setCodgrupoprod] = useState<string>(searchParams.get('codgrupoprod') || 'all');
  const [codlocal, setCodlocal] = useState<string>(searchParams.get('codlocal') || 'all');
  const [usoprod, setUsoprod] = useState<'C' | 'V' | 'S' | 'all'>((searchParams.get('usoprod') as any) || 'C');
  const [page, setPage] = useState(Number(searchParams.get('page')) || 1);
  const [perPage, setPerPage] = useState(Number(searchParams.get('perPage')) || 20);
  const [pageInput, setPageInput] = useState('');

  // Listas para filtros
  const [grupos, setGrupos] = useState<GrupoProduto[]>([]);
  const [locais, setLocais] = useState<LocalEstoque[]>([]);

  // Carregar stats, grupos e locais na montagem do componente
  useEffect(() => {
    loadStats();
    loadGrupos();
    loadLocais();
  }, []);

  // Atualizar URL quando filtros mudarem
  useEffect(() => {
    const params = new URLSearchParams();
    if (search) params.set('search', search);
    if (ativo !== 'all') params.set('ativo', ativo);
    if (codgrupoprod !== 'all') params.set('codgrupoprod', codgrupoprod);
    if (codlocal !== 'all') params.set('codlocal', codlocal);
    if (usoprod !== 'all') params.set('usoprod', usoprod);
    if (page > 1) params.set('page', page.toString());
    if (perPage !== 20) params.set('perPage', perPage.toString());
    setSearchParams(params);
    loadData();
  }, [page, ativo, codgrupoprod, codlocal, usoprod, perPage]);

  async function loadStats() {
    try {
      const statsResult = await getStatsProdutosSemNCM();
      setStats(statsResult);
    } catch (err: any) {
      console.error('Erro ao carregar estatísticas:', err);
    }
  }

  async function loadGrupos() {
    try {
      const gruposResult = await getGruposProdutos();
      setGrupos(gruposResult);
    } catch (err: any) {
      console.error('Erro ao carregar grupos:', err);
    }
  }

  async function loadLocais() {
    try {
      const locaisResult = await getLocaisEstoque();
      setLocais(locaisResult);
    } catch (err: any) {
      console.error('Erro ao carregar locais:', err);
    }
  }

  async function loadData() {
    try {
      setLoading(true);
      setError(null);

      const filters: ProdutosSemNCMFilters = {
        page,
        perPage,
      };

      if (search) filters.search = search;
      if (ativo !== 'all') filters.ativo = ativo;
      if (codgrupoprod !== 'all') filters.codgrupoprod = Number(codgrupoprod);
      if (codlocal !== 'all') filters.codlocal = Number(codlocal);
      if (usoprod !== 'all') filters.usoprod = usoprod;

      const result = await getProdutosSemNCM(filters);
      setData(result);
    } catch (err: any) {
      console.error('Erro ao carregar produtos sem NCM:', err);
      setError(err.message || 'Erro ao carregar dados. Tente novamente.');
    } finally {
      setLoading(false);
    }
  }

  function handleSearchSubmit(e: React.FormEvent) {
    e.preventDefault();
    setPage(1);
    loadData();
  }

  function clearFilters() {
    setSearch('');
    setAtivo('S');
    setCodgrupoprod('all');
    setCodlocal('all');
    setUsoprod('C');
    setPage(1);
  }

  const hasActiveFilters = search || ativo !== 'S' || codgrupoprod !== 'all' || codlocal !== 'all' || usoprod !== 'C';

  if (error) {
    return (
      <BaseLayout>
        <div className="flex-1 space-y-4 p-4 md:p-8 pt-6">
          <div className="flex flex-col items-center justify-center min-h-[400px] gap-4">
            <AlertTriangle className="h-12 w-12 text-destructive" />
            <p className="text-lg text-muted-foreground">{error}</p>
            <Button onClick={loadData}>Tentar Novamente</Button>
          </div>
        </div>
      </BaseLayout>
    );
  }

  const percentualCriticos = stats ? calcularPercentual(stats.totalCriticosAlta, stats.totalAtivos) : 0;

  return (
    <BaseLayout>
      <div className="flex-1 space-y-4 p-4 md:p-8 pt-6">
        {/* Header */}
        <div className="flex items-center justify-between">
          <div>
            <h2 className="text-3xl font-bold tracking-tight">Produtos sem NCM</h2>
            <p className="text-muted-foreground">
              Controle de qualidade de dados para compliance fiscal
            </p>
          </div>
        </div>

        {/* KPI Cards - Clicáveis para filtrar */}
        {stats && (
          <div className="grid gap-4 md:grid-cols-4">
            {/* Card: Criticidade ALTA - Clicável */}
            <Card
              className={`border-2 cursor-pointer transition-all hover:shadow-lg ${
                criticidade === 'ALTA' && ativo === 'all' ? 'ring-2 ring-orange-500 border-orange-500' : ''
              }`}
              onClick={() => {
                if (criticidade === 'ALTA' && ativo === 'all') {
                  setCriticidade('all');
                } else {
                  setCriticidade('ALTA');
                  setAtivo('all');
                  setPage(1);
                }
              }}
            >
              <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                <CardTitle className="text-sm font-medium">Criticidade ALTA</CardTitle>
                <AlertTriangle className="h-4 w-4 text-orange-500" />
              </CardHeader>
              <CardContent>
                <div className="text-2xl font-bold text-orange-500">
                  {stats.totalCriticosAlta.toLocaleString('pt-BR')}
                </div>
                <p className="text-xs text-muted-foreground mt-1">
                  Ativos sem NCM ({percentualCriticos.toFixed(1)}%)
                </p>
                <div className="mt-2 flex items-center gap-2">
                  <div className="flex-1 h-2 bg-secondary rounded-full overflow-hidden">
                    <div
                      className="h-full bg-orange-500"
                      style={{ width: `${percentualCriticos}%` }}
                    />
                  </div>
                </div>
                <p className="text-xs text-muted-foreground mt-2">
                  {criticidade === 'ALTA' ? '✓ Filtro ativo' : 'Clique para filtrar'}
                </p>
              </CardContent>
            </Card>

            {/* Card: Ativos - Clicável */}
            <Card
              className={`border-2 cursor-pointer transition-all hover:shadow-lg ${
                ativo === 'S' && criticidade === 'all' ? 'ring-2 ring-blue-500 border-blue-500' : ''
              }`}
              onClick={() => {
                if (ativo === 'S' && criticidade === 'all') {
                  setAtivo('all');
                } else {
                  setAtivo('S');
                  setCriticidade('all');
                  setPage(1);
                }
              }}
            >
              <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                <CardTitle className="text-sm font-medium">Somente Ativos</CardTitle>
                <CheckCircle className="h-4 w-4 text-blue-500" />
              </CardHeader>
              <CardContent>
                <div className="text-2xl font-bold text-blue-500">
                  {stats.totalAtivos.toLocaleString('pt-BR')}
                </div>
                <p className="text-xs text-muted-foreground mt-1">
                  {calcularPercentual(stats.totalAtivos, stats.total).toFixed(1)}% do total
                </p>
                <div className="mt-2 flex items-center gap-2">
                  <div className="flex-1 h-2 bg-secondary rounded-full overflow-hidden">
                    <div
                      className="h-full bg-blue-500"
                      style={{ width: `${calcularPercentual(stats.totalAtivos, stats.total)}%` }}
                    />
                  </div>
                </div>
                <p className="text-xs text-muted-foreground mt-2">
                  {ativo === 'S' ? '✓ Filtro ativo' : 'Clique para filtrar'}
                </p>
              </CardContent>
            </Card>

            {/* Card: Inativos - Clicável */}
            <Card
              className={`border-2 cursor-pointer transition-all hover:shadow-lg ${
                ativo === 'N' && criticidade === 'all' ? 'ring-2 ring-gray-500 border-gray-500' : ''
              }`}
              onClick={() => {
                if (ativo === 'N' && criticidade === 'all') {
                  setAtivo('all');
                } else {
                  setAtivo('N');
                  setCriticidade('all');
                  setPage(1);
                }
              }}
            >
              <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                <CardTitle className="text-sm font-medium">Somente Inativos</CardTitle>
                <Package className="h-4 w-4 text-gray-500" />
              </CardHeader>
              <CardContent>
                <div className="text-2xl font-bold text-gray-500">
                  {stats.totalInativos.toLocaleString('pt-BR')}
                </div>
                <p className="text-xs text-muted-foreground mt-1">
                  {calcularPercentual(stats.totalInativos, stats.total).toFixed(1)}% do total
                </p>
                <div className="mt-2 flex items-center gap-2">
                  <div className="flex-1 h-2 bg-secondary rounded-full overflow-hidden">
                    <div
                      className="h-full bg-gray-500"
                      style={{
                        width: `${calcularPercentual(stats.totalInativos, stats.total)}%`,
                      }}
                    />
                  </div>
                </div>
                <p className="text-xs text-muted-foreground mt-2">
                  {ativo === 'N' ? '✓ Filtro ativo' : 'Clique para filtrar'}
                </p>
              </CardContent>
            </Card>

            {/* Card: Todos - Clicável para limpar filtros */}
            <Card
              className={`border-2 cursor-pointer transition-all hover:shadow-lg ${
                !hasActiveFilters ? 'ring-2 ring-primary border-primary' : ''
              }`}
              onClick={clearFilters}
            >
              <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
                <CardTitle className="text-sm font-medium">Todos os Produtos</CardTitle>
                <Filter className="h-4 w-4 text-primary" />
              </CardHeader>
              <CardContent>
                <div className="text-2xl font-bold">{stats.total.toLocaleString('pt-BR')}</div>
                <p className="text-xs text-muted-foreground mt-1">
                  Total sem NCM cadastrado
                </p>
                <div className="mt-2 flex items-center gap-2">
                  <div className="flex-1 h-2 bg-secondary rounded-full overflow-hidden">
                    <div className="h-full bg-primary" style={{ width: '100%' }} />
                  </div>
                </div>
                <p className="text-xs text-muted-foreground mt-2">
                  {!hasActiveFilters ? '✓ Todos visíveis' : 'Clique para limpar filtros'}
                </p>
              </CardContent>
            </Card>
          </div>
        )}

        {/* Filtros */}
        <Card>
          <CardHeader>
            <CardTitle className="flex items-center gap-2">
              <Filter className="h-5 w-5" />
              Filtros
            </CardTitle>
            <CardDescription>Refine sua busca com filtros avançados</CardDescription>
          </CardHeader>
          <CardContent>
            <form onSubmit={handleSearchSubmit} className="space-y-4">
              <div className="grid gap-4 md:grid-cols-4">
                {/* Search */}
                <div className="md:col-span-2">
                  <label className="text-sm font-medium mb-2 block">Buscar</label>
                  <div className="relative">
                    <Search className="absolute left-3 top-3 h-4 w-4 text-muted-foreground" />
                    <Input
                      placeholder="Descrição, referência ou marca..."
                      value={search}
                      onChange={(e) => setSearch(e.target.value)}
                      className="pl-9"
                    />
                  </div>
                </div>

                {/* Ativo */}
                <div>
                  <label className="text-sm font-medium mb-2 block">Status</label>
                  <Select value={ativo} onValueChange={(v) => setAtivo(v as any)}>
                    <SelectTrigger>
                      <SelectValue />
                    </SelectTrigger>
                    <SelectContent>
                      <SelectItem value="all">Todos</SelectItem>
                      <SelectItem value="S">Ativos</SelectItem>
                      <SelectItem value="N">Inativos</SelectItem>
                    </SelectContent>
                  </Select>
                </div>

                {/* Tipo Uso */}
                <div>
                  <label className="text-sm font-medium mb-2 block">Tipo Uso</label>
                  <Select value={usoprod} onValueChange={(v) => setUsoprod(v as any)}>
                    <SelectTrigger>
                      <SelectValue />
                    </SelectTrigger>
                    <SelectContent>
                      <SelectItem value="all">Todos</SelectItem>
                      <SelectItem value="C">Consumo</SelectItem>
                      <SelectItem value="V">Venda</SelectItem>
                      <SelectItem value="S">Serviço</SelectItem>
                    </SelectContent>
                  </Select>
                </div>
              </div>

              <div className="grid gap-4 md:grid-cols-2">
                {/* Grupo */}
                <div>
                  <label className="text-sm font-medium mb-2 block">Grupo</label>
                  <Select value={codgrupoprod} onValueChange={(v) => setCodgrupoprod(v)}>
                    <SelectTrigger>
                      <SelectValue placeholder="Todos os grupos" />
                    </SelectTrigger>
                    <SelectContent>
                      <SelectItem value="all">Todos os grupos</SelectItem>
                      {grupos.map((grupo) => (
                        <SelectItem key={grupo.codgrupoprod} value={grupo.codgrupoprod.toString()}>
                          {grupo.descrgrupoprod}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                </div>

                {/* Local */}
                <div>
                  <label className="text-sm font-medium mb-2 block">Local</label>
                  <Select value={codlocal} onValueChange={(v) => setCodlocal(v)}>
                    <SelectTrigger>
                      <SelectValue placeholder="Todos os locais" />
                    </SelectTrigger>
                    <SelectContent>
                      <SelectItem value="all">Todos os locais</SelectItem>
                      {locais.map((local) => (
                        <SelectItem key={local.codlocal} value={local.codlocal.toString()}>
                          {local.descrlocal}
                        </SelectItem>
                      ))}
                    </SelectContent>
                  </Select>
                </div>
              </div>

              <div className="flex gap-2">
                <Button type="submit">
                  <Search className="h-4 w-4 mr-2" />
                  Buscar
                </Button>
                {hasActiveFilters && (
                  <Button type="button" variant="outline" onClick={clearFilters}>
                    <X className="h-4 w-4 mr-2" />
                    Limpar Filtros
                  </Button>
                )}
              </div>
            </form>
          </CardContent>
        </Card>

        {/* Tabela */}
        <Card>
          <CardHeader>
            <CardTitle>Produtos</CardTitle>
            <CardDescription>
              {data ? `${data.total} produtos encontrados` : 'Carregando...'}
            </CardDescription>
          </CardHeader>
          <CardContent>
            {loading ? (
              <div className="flex items-center justify-center py-12">
                <Loader2 className="h-8 w-8 animate-spin text-primary" />
              </div>
            ) : data && data.produtos.length > 0 ? (
              <>
                <div className="overflow-x-auto">
                  <Table>
                    <TableHeader>
                      <TableRow>
                        <TableHead className="w-[80px]">Código</TableHead>
                        <TableHead className="min-w-[250px]">Descrição</TableHead>
                        <TableHead className="min-w-[150px]">Grupo</TableHead>
                        <TableHead>Marca</TableHead>
                        <TableHead>Tipo Uso</TableHead>
                        <TableHead className="w-[100px]">Cód. Local</TableHead>
                        <TableHead className="min-w-[150px]">Local</TableHead>
                        <TableHead>Localização</TableHead>
                        <TableHead>Usa Local</TableHead>
                        <TableHead>Status</TableHead>
                        <TableHead>Criticidade</TableHead>
                        <TableHead className="min-w-[150px]">Usuário</TableHead>
                        <TableHead className="text-right w-[120px]">Ações</TableHead>
                      </TableRow>
                    </TableHeader>
                    <TableBody>
                      {data.produtos.map((produto) => (
                        <TableRow key={produto.codprod}>
                          <TableCell className="font-medium">{produto.codprod}</TableCell>
                          <TableCell>
                            <div>
                              <div className="font-medium">{produto.descrprod}</div>
                              {produto.referencia && (
                                <div className="text-sm text-muted-foreground">
                                  Ref: {produto.referencia}
                                </div>
                              )}
                              {produto.compldesc && (
                                <div className="text-sm text-muted-foreground">
                                  {produto.compldesc}
                                </div>
                              )}
                            </div>
                          </TableCell>
                          <TableCell>
                            {produto.tgfgru ? (
                              <Link
                                to={`/produtos-v2/grupo/${produto.codgrupoprod}`}
                                className="text-sm hover:underline"
                              >
                                {produto.tgfgru.descrgrupoprod}
                              </Link>
                            ) : (
                              <span className="text-sm text-muted-foreground">-</span>
                            )}
                          </TableCell>
                          <TableCell>{produto.marca || '-'}</TableCell>
                          <TableCell>
                            <Badge variant="outline">
                              {produto.usoprod === 'C' ? 'Consumo' : produto.usoprod === 'V' ? 'Venda' : produto.usoprod === 'S' ? 'Serviço' : '-'}
                            </Badge>
                          </TableCell>
                          <TableCell className="text-sm text-center">
                            {produto.codlocalpadrao ? (
                              <Link
                                to={`/produtos-v2/local/${produto.codlocalpadrao}`}
                                className="hover:underline font-medium"
                              >
                                {produto.codlocalpadrao}
                              </Link>
                            ) : (
                              '-'
                            )}
                          </TableCell>
                          <TableCell className="text-sm">
                            {produto.tgfloc?.descrlocal || '-'}
                          </TableCell>
                          <TableCell className="text-sm">
                            {produto.localizacao || '-'}
                          </TableCell>
                          <TableCell>
                            <Badge variant={produto.usalocal === 'S' ? 'default' : 'secondary'}>
                              {produto.usalocal === 'S' ? 'Sim' : 'Não'}
                            </Badge>
                          </TableCell>
                          <TableCell>
                            <Badge variant={produto.ativo === 'S' ? 'default' : 'secondary'}>
                              {produto.ativo === 'S' ? 'Ativo' : 'Inativo'}
                            </Badge>
                          </TableCell>
                          <TableCell>
                            <Badge
                              variant={
                                produto.criticidade === 'ALTA'
                                  ? 'destructive'
                                  : produto.criticidade === 'MEDIA'
                                    ? 'default'
                                    : 'secondary'
                              }
                            >
                              {produto.criticidade}
                            </Badge>
                          </TableCell>
                          <TableCell className="text-sm">
                            {produto.nomeusu ? (
                              produto.dtlimacesso ? (
                                <TooltipProvider>
                                  <Tooltip>
                                    <TooltipTrigger asChild>
                                      <div className="cursor-help">
                                        <div className="text-gray-400">
                                          {produto.nomeusu}
                                        </div>
                                        <div className="text-xs text-gray-400">
                                          Desativado
                                        </div>
                                      </div>
                                    </TooltipTrigger>
                                    <TooltipContent>
                                      <p>Desativado em: {new Date(produto.dtlimacesso).toLocaleDateString('pt-BR')}</p>
                                    </TooltipContent>
                                  </Tooltip>
                                </TooltipProvider>
                              ) : (
                                <div>{produto.nomeusu}</div>
                              )
                            ) : (
                              '-'
                            )}
                          </TableCell>
                          <TableCell className="text-right">
                            <Link to={`/produtos-v2/${produto.codprod}`}>
                              <Button variant="ghost" size="sm">
                                Ver Detalhes
                              </Button>
                            </Link>
                          </TableCell>
                        </TableRow>
                      ))}
                    </TableBody>
                  </Table>
                </div>

                {/* Paginação Avançada */}
                <div className="flex flex-col gap-4 mt-4">
                  <div className="flex items-center justify-between">
                    <div className="flex items-center gap-4">
                      <div className="text-sm text-muted-foreground">
                        Página {data.page} de {data.lastPage} • Total: {data.total} produtos
                      </div>
                      <div className="flex items-center gap-2">
                        <label className="text-sm text-muted-foreground">Itens por página:</label>
                        <Select
                          value={perPage.toString()}
                          onValueChange={(v) => {
                            setPerPage(Number(v));
                            setPage(1);
                          }}
                        >
                          <SelectTrigger className="w-[80px]">
                            <SelectValue />
                          </SelectTrigger>
                          <SelectContent>
                            <SelectItem value="10">10</SelectItem>
                            <SelectItem value="20">20</SelectItem>
                            <SelectItem value="50">50</SelectItem>
                            <SelectItem value="100">100</SelectItem>
                          </SelectContent>
                        </Select>
                      </div>
                    </div>
                    <div className="flex items-center gap-2">
                      <Button
                        variant="outline"
                        size="sm"
                        onClick={() => setPage(1)}
                        disabled={page === 1}
                      >
                        Primeira
                      </Button>
                      <Button
                        variant="outline"
                        size="sm"
                        onClick={() => setPage(page - 1)}
                        disabled={page === 1}
                      >
                        <ChevronLeft className="h-4 w-4" />
                      </Button>
                      <div className="flex items-center gap-2">
                        <Input
                          type="number"
                          min="1"
                          max={data.lastPage}
                          placeholder={page.toString()}
                          value={pageInput}
                          onChange={(e) => setPageInput(e.target.value)}
                          onKeyDown={(e) => {
                            if (e.key === 'Enter') {
                              const newPage = Number(pageInput);
                              if (newPage >= 1 && newPage <= data.lastPage) {
                                setPage(newPage);
                                setPageInput('');
                              }
                            }
                          }}
                          className="w-[80px] text-center"
                        />
                        <span className="text-sm text-muted-foreground">de {data.lastPage}</span>
                      </div>
                      <Button
                        variant="outline"
                        size="sm"
                        onClick={() => setPage(page + 1)}
                        disabled={!data.hasMore}
                      >
                        <ChevronRight className="h-4 w-4" />
                      </Button>
                      <Button
                        variant="outline"
                        size="sm"
                        onClick={() => setPage(data.lastPage)}
                        disabled={page === data.lastPage}
                      >
                        Última
                      </Button>
                    </div>
                  </div>
                </div>
              </>
            ) : (
              <div className="text-center py-12 text-muted-foreground">
                {hasActiveFilters
                  ? 'Nenhum produto encontrado com os filtros selecionados.'
                  : 'Nenhum produto sem NCM encontrado!'}
              </div>
            )}
          </CardContent>
        </Card>

        {/* Help Card */}
        <Card className="border-primary">
          <CardHeader>
            <CardTitle className="flex items-center gap-2">
              <AlertTriangle className="h-5 w-5 text-primary" />
              Por que o NCM é importante?
            </CardTitle>
          </CardHeader>
          <CardContent className="space-y-2 text-sm text-muted-foreground">
            <p>
              <strong>Compliance Fiscal:</strong> O NCM (Nomenclatura Comum do Mercosul) é
              obrigatório para emissão de notas fiscais eletrônicas (NF-e).
            </p>
            <p>
              <strong>Tributação:</strong> Determina alíquotas de impostos (ICMS, IPI, PIS, COFINS).
            </p>
            <p>
              <strong>Penalidades:</strong> Produtos sem NCM podem gerar rejeição de NF-e e multas
              fiscais.
            </p>
          </CardContent>
        </Card>
      </div>
    </BaseLayout>
  );
}
